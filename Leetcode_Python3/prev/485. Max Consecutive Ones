class Solution:
    def findMaxConsecutiveOnes(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        cnt = 0
        big = 0
        for num in nums:
            if num == 1:           
                cnt += 1
                big = max(big,cnt)
            else:
                num == 0
                cnt = 0
        return big;
                
