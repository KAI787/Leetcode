class Solution:
    def matrixReshape(self, nums, r, c):
        """
        :type nums: List[List[int]]
        :type r: int
        :type c: int
        :rtype: List[List[int]]
        """
        if(len(nums)*len(nums[0]) != r*c):
            return nums
        else:
            vals = (val for row in nums for val in row)
            return [[next(vals) for x in range(c)] for y in range(r)]
